{
  "name": "actions-docup",
  "version": "1.0.0",
  "description": "A github action for docup",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "ncc run src/index.ts",
    "prepare": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "docup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/io": "^1.1.2",
    "tempura": "0.4.0"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.0.0",
    "typescript": "4.8.4",
    "@typescript-eslint/parser": "5.40.0",
    "@typescript-eslint/eslint-plugin": "5.40.0"
  }
}
